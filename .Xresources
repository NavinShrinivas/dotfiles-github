!--- Window Settings
Xcursor.size:11
URxvt.buffered:             true

!--- Font Size and Controls
URxvt*.font:  xft:JetBrainsMono NF:size=18
!URxvt.perl-ext-common: font-size,tabbed,matcher
URxvt.geometry: 92x24

!--- For Tabs
URxvt.tabbed.reopen-on-close: yes
URxvt.keysym.Control-Shift-Left: perl:tabbedex:move_tab_left
URxvt.keysym.Control-Shift-Right: perl:tabbedex:move_tab_right
URxvt.keysym.Control-Shift-R: perl:tabbedex:rename_tab


!--- Scrolling
URxvt*.scrollTtyOutput:     false
URxvt*.scrollWithBuffer:    false
URxvt*.scrollTtyKeypress:   true
URxvt.secondaryScroll:      true
URxvt*scrollstyle:          plain
URxvt*saveLines:            10000
URxvt.scrollBar_right:      true
URxvt.scrollBar:            false
URxvt.mapAlert:             true

!
! Ayu Dark
!   Dark version with Mirage as colors 8-15
!
! Appearance

urxvt*termName:         rxvt
urxvt*depth:            32
urxvt*lineSpace:	    4
urxvt*internalBorder:   16

! Custom
! Original theme https://github.com/sindresorhus/hyper-snazzy
*.foreground:    #eff0eb
*.background:    #282a36
*.cursorColor:   #97979b
*.highlightColor:#3A3A3A

! Negro y gris
*.color0:        #282a36
*.color8:        #686868
! Rojos
*.color1:        #ff5c57
*.color9:        #ff5c57
! Verde
*.color2:        #5af78e
*.color10:       #5af78e
! Amarillo
*.color3:        #f3f99d
*.color11:       #f3f99d
! Azul
*.color4:        #57c7ff
*.color12:       #57c7ff
! Magenta
*.color5:        #ff6ac1
*.color13:       #ff6ac1
! Cyan
*.color6:        #9aedfe
*.color14:       #9aedfe
! Blanco
*.color7:        #f1f1f0
*.color15:       #eff0eburxvt*transparent: true
urxvt*shading: 40

urxvt.perl-lib:        /home/navin/urxvt-perls
URxvt.perl-ext-common: keyboard-select,resize-font

URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:      resize-font:bigger

